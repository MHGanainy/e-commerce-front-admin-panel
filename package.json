{
  "name": "ecommerce-frontend-new",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --host 0.0.0.0 --prod  --port 4200 --disable-host-check ",
    "nghm": "node --max_old_space_size=2500 ./node_modules/@angular/cli/bin/ng",
    "build": "npm run nghm -- build --prod --aot=true",
    "build:stats": "ng build --prod --stats-json",
    "generateDoc": "compodoc -p tsconfig.json -s",
    "analyze": "node_modules/.bin/webpack-bundle-analyzer-brotli dist/ecommerce-frontend-new/stats.json",
    "serveDev": "ng serve --host tmgdevcic.com --disable-host-check",
    "serveDev4201": "ng serve --host tmgdevcic.com --disable-host-check --port 4201",
    "serveDevProdEnv": "ng serve -c prodEnv --host tmgdevcic.com --disable-host-check",
    "serveDevwProxy": "ng serve -c offline --host tmgdevcic.com --disable-host-check --proxy-config proxy.conf.json",
    "serveProd": "npm run nghm -- serve --prod --host tmgdevcic.com",
    "serveDevLocal": "ng serve -c offline --host tmgdevcic.com --port 4202 --disable-host-check",
    "serveOff": "ng serve -c offline",
    "serveO": "ng serve --host tmgdevcic.com --disable-host-check --o",
    "optimizecss": "gulp --gulpfile optimize-css.gulpfile.js",
    "buildOpt": "npm run build"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "angular-alternative",
      "options": {}
    }
  },
